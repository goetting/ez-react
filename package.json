{
  "name": "ez-react",
  "version": "0.9.5",
  "description": "A small react-connector for ez-flux",
  "main": "lib/index.js",
  "scripts": {
    "start": "bash start.sh",
    "build": "babel src --out-dir lib",
    "build:watch": "npm run build -- --watch",
    "flow:watch": "nodemon -q --watch src --test --ext js --ext jsx -x 'flow; echo'",
    "lint:fix": "eslint --color --fix --ext .js ./src",
    "lint:watch": "esw -w --cache --color --ext .js --ext .jsx ./src ./test",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goetting/ez-react.git"
  },
  "keywords": [
    "react",
    "flux",
    "ez-flux",
    "connector",
    "bridge",
    "state",
    "machine"
  ],
  "preerDependencies": {
    "react": ">15.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "concurrently": "^3.5.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "eslint": "3.13.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "2.29.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^3.1.3",
    "ez-flux": "^0.11.1",
    "flow-bin": "^0.57.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "webpack": "^2.7.0"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ]
  },
  "author": "Goetz Goetting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goetting/ez-react/issues"
  },
  "homepage": "https://github.com/goetting/ez-react#readme",
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
